{
    "name": "orchestra/dusk-updater-api",
    "description": "Updater API for Laravel Dusk ChromeDriver binaries",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Staudenmeir",
            "email": "mail@jonas-staudenmeir.de"
        },
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\DuskUpdaterApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "ext-openssl": "*",
        "composer-runtime-api": "^2.2",
        "guzzlehttp/guzzle": "^7.2",
        "orchestra/sidekick": "^1.0.2",
        "symfony/polyfill-ctype": "^1.9",
        "symfony/process": "^6.0|^7.0"
    },
    "require-dev": {
        "laravel/pint": "^1.4",
        "mockery/mockery": "^1.5.1",
        "pestphp/pest": "^1.0|^2.0|^3.0",
        "phpstan/phpstan": "^2.1"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "lint": [
            "@php vendor/bin/pint",
            "@php vendor/bin/phpstan analyse --verbose"
        ],
        "test": "@php vendor/bin/pest",
        "ci": [
            "@lint",
            "@test"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
