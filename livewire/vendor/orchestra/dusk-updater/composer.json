{
    "name": "orchestra/dusk-updater",
    "description": "Updater for Laravel Dusk ChromeDriver binaries",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Staudenmeir",
            "email": "mail@jonas-staudenmeir.de"
        },
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "bin": [
        "dusk-updater"
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\DuskUpdater\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-openssl": "*",
        "ext-zip": "*",
        "composer-runtime-api": "^2.2",
        "composer/semver": "^3.0",
        "guzzlehttp/guzzle": "^7.9",
        "laravel/prompts": "^0.1.25 || ^0.2 || ^0.3",
        "orchestra/sidekick": "^1.0",
        "orchestra/dusk-updater-api": "^1.0.2",
        "symfony/console": "^6.2 || ^7.0",
        "symfony/process": "^6.2 || ^7.0"
    },
    "require-dev": {
        "laravel/dusk": "^8.0",
        "laravel/pint": "^1.20",
        "pestphp/pest": "^2.0 || ^3.0",
        "phpstan/phpstan": "^2.1"
    },
    "conflict": {
        "laravel/dusk": "<8.0.0 || >=9.0.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "lint": [
            "@php vendor/bin/pint",
            "@php vendor/bin/phpstan analyse --verbose"
        ],
        "test": "@php vendor/bin/pest",
        "ci": [
            "@lint",
            "@test"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
